<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<LinearGradientBrush x:Key="BorderTransparentColor" StartPoint=".5,0" EndPoint=".5,1">
		<GradientStop Color="#029BBA" Offset="0" />
		<GradientStop Color="#027892" Offset="1" />
	</LinearGradientBrush>
	<Color x:Key="TitleColor">#44000000</Color>
	<SolidColorBrush x:Key="BackgroundColor">#DBF0FE</SolidColorBrush>
	<LinearGradientBrush x:Key="MouseOverColor" StartPoint="0.5,0" EndPoint="0.5,1">
		<GradientStop Color="#0AA0BE" Offset="0" />
		<GradientStop Color="LightBlue" Offset="1" />
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="TitlePanelsBackgroundColor" EndPoint="0.5,1" StartPoint="0.5,0">
		<GradientStop Color="#FF009AB7" Offset="0" />
		<GradientStop Color="#FF0085A7" Offset="1" />
	</LinearGradientBrush>
</ResourceDictionary>